<div class="sign-in-page">

<h1 class="test group">Sign In</h1>

<%= form_tag sessions_url do %>
  <div class="email">
    <%= label_tag :email %>
    <%= text_field_tag :email, @user.email %>
  </div>

  <div class="password">
    <%= label_tag :password, "What's your password?" %>
    <%= password_field_tag :password %>
  </div>
  
  <div class="submit">
  <%= submit_tag "Sign In" %>
 </div>

<% end %>
